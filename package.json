{
  "name": "webapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 80",
    "build": "next build",
    "start": "next start -p 80",
    "lint": "next lint --fix",
    "analyze": "ANALYZE=true yarn build",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:e2e": "DEBUG=pw:webserver playwright test",
    "lighthouse": "lighthouse http://localhost --quiet --chrome-flags='--headless' --output=html --output-path=lh-report.html --budget-path=budget.json --formFactor=desktop --no-'screenEmulation.mobile' --screenEmulation.width=1280 --screenEmulation.height=600 --screenEmulation.deviceScaleFactor=2",
    "lighthouse:authed": "node ./lighthouse.js http://localhost/app/profile --login --quiet --output=html --output-path=lh-report.html --budget-path=budget.json --formFactor=desktop --no-'screenEmulation.mobile' --screenEmulation.width=1280 --screenEmulation.height=600 --screenEmulation.deviceScaleFactor=2"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "next": "12.1.6",
    "react": "^18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.15.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.1",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/supertest": "^2.0.12",
    "autoprefixer": "^10.4.7",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-xs": "^1.2.1",
    "eslint-plugin-testing-library": "^5.5.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lighthouse": "^9.6.2",
    "node-mocks-http": "^1.11.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.2",
    "typeface-muli": "^1.1.13",
    "typescript": "4.7.4"
  }
}
