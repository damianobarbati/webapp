    "lighthouse": "CI=true lighthouse http://localhost --quiet --chrome-flags='--headless' --output=html --output-path=lh-report.html --budget-path=budget.json --formFactor=desktop --no-'screenEmulation.mobile' --screenEmulation.width=1280 --screenEmulation.height=600 --screenEmulation.deviceScaleFactor=2",
